{
    "name": "r-vscode-tools",
    "displayName": "r-vscode-tools",
    "description": "An extension to perform R CMD check on packages",
    "version": "0.0.1",
    "publisher": "hkumar6",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Languages"
    ],
    "activationEvents": [
        "onCommand:extension.checkRpackage"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.checkRpackage",
            "title": "R: Check package"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "shelljs": "^0.7",
        "@types/mocha": "^2.2.32"
    }
}